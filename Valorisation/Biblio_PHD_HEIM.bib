@article{BergemannVanDenBerg2008,
  title = {Active {{Labor Market Policy Effects}} for {{Women}} in {{Europe}} — {{A Survey}}},
  author = {Bergemann, Annette and Van Den Berg, Gerard J.},
  date = {2008},
  journaltitle = {Annales d'Économie et de Statistique},
  number = {91/92},
  pages = {385--408},
  publisher = {[GENES, ADRES]},
  abstract = {We survey the recent literature on the effects of active labor market policies on individual labor market outcomes like employment and income, for adult female individuals without work in European countries. We consider skill-training programs, monitoring and sanctions, job search assistance, and employment subsidies. The results are remarkably uniform across studies. We relate the results to the relevant level of female labor force participation. Nous présentons la littérature récente sur les effets des politiques de marchés du travail sur les performances individuelles sur le marché du travail, telles que l'obtention d'un emploi ou le salaire, pour des femmes adultes sans travail dans les pays Européens. Nous examinons les programmes de formations, le contrôle et les sanctions, l'assistance à la recherche d'emploi, et les subventions à l'emploi. Les résultats sont remarquablement uniformes d'un pays à l'autre. Nous lions les résultats au niveau pertinent de participation des femmes au marché du travail.}
}

