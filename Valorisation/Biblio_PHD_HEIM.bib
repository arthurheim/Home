@article{MartinDutreuilh2022,
  title = {Changes in Labour Force Participation and Employment Rates Across Successive Cohorts in France},
  author = {Martin, Henri and Dutreuilh, Catriona},
  date = {2022-06-24},
  journaltitle = {Population},
  volume = {77},
  number = {1},
  pages = {137--154},
  publisher = {I.N.E.D},
  abstract = {En France, la forte progression de la participation des femmes au march\&\#233; du travail depuis les ann\&\#233;es 1970 est un constat d\&\#233;j\&\#224; bien \&\#233;tabli dans la litt\&\#233;rature \&\#233;conomique. En mobilisant les enqu\&\#234;tes Emploi de l\&\#8217;Insee sur une p\&\#233;riode longue de 44\&\#160;ans couvrant les ann\&\#233;es 1975 \&\#224; 2018, cette contribution s\&\#8217;int\&\#233;resse aux \&\#233;volutions des taux d\&\#8217;activit\&\#233; et d\&\#8217;emploi des femmes et des hommes. Alors que l\&\#8217;activit\&\#233; et l\&\#8217;emploi des femmes entre 25 et 50\&\#160;ans n\&\#8217;avaient cess\&\#233; de progresser au fil des g\&\#233;n\&\#233;rations depuis celle n\&\#233;e en 1920, les donn\&\#233;es font \&\#233;tat d\&\#8217;une stagnation pour les femmes n\&\#233;es apr\&\#232;s 1970. Concernant les hommes, activit\&\#233; et emploi \&\#224; ces \&\#226;ges tendent \&\#224; reculer l\&\#233;g\&\#232;rement au fil des cohortes. Les \&\#233;carts de taux d\&\#8217;activit\&\#233; et de taux d\&\#8217;emploi entre femmes et hommes continuent de se r\&\#233;sorber, mais \&\#224; un rythme de plus en plus lent. Alors que ce processus de rattrapage \&\#233;tait, pour les g\&\#233;n\&\#233;rations n\&\#233;es avant 1970, essentiellement la cons\&\#233;quence de l\&\#8217;augmentation de ces indicateurs chez les femmes, il est d\&\#233;sormais int\&\#233;gralement d\&\#251; \&\#224; leur diminution chez les hommes.},
  annotation = {Bibliographie\_available: 1\\
Cairndomain: www.cairn-int.info\\
Cite Par\_available: 0}
}

@article{MartinezEtAl2022,
  title = {Structural Gender Inequalities and Symptoms of Postpartum Depression in 40 Countries},
  author = {Martínez, Pablo and Nazif-Munoz, José Ignacio and Rojas, Graciela and Magaña, Irene},
  date = {2022-01-15},
  journaltitle = {Journal of Affective Disorders},
  shortjournal = {Journal of Affective Disorders},
  volume = {297},
  pages = {381--385},
  abstract = {Background The role of structural gender inequality in macro-level differences in women's perinatal mental health remains largely unexplored. This short communication explores structural gender inequalities and their potential as a macro-level, upstream social determinant of postpartum depression (PPD). Methods We compiled meta-analytically derived national-level prevalence estimates of PPD symptoms – based on the Edinburgh Postnatal Depression Scale – with economic (e.g., income inequality), health (e.g., infant mortality rate), sociodemographic (e.g., urban population), and structural gender inequality variables (e.g., abortion policies) for 40 countries. Meta-regression techniques and traditional p-value based stepwise procedures, complemented with a Bayesian model averaging approach, were used for a robust selection of variables associated with national-level PPD symptom prevalence. Results Income inequality (β~=~0.04, 95\% CI~=~0.02 to 0.07) and abortion policies (β~=~0.02, 95\% CI~=~0.00 to 0.03) were the only variables selected in the final, adjusted model, accounting for 60.7\% of cross-national variations in PPD symptoms. Limitations Study quality of primary studies was not assessed and some national-level meta-analytical estimates were based on few primary studies. A fifth of world countries and territories could be included, with high-income regions overrepresented. High rate of missing national-level data for potential predictors of PPD. Cross-sectional analyses precludes causal inferences. Conclusions Abortion policies are a significant macro-level social determinant of PPD, and its liberalization might be associated with women's mental health at a population level. Our findings should be a relevant argument for clinicians to advocate for changing discriminatory social norms against women.}
}

