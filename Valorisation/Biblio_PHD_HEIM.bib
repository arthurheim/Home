@article{MuradEtAl2016,
  title = {New Evidence Pyramid},
  author = {Murad, M. Hassan and Asi, Noor and Alsawas, Mouaz and Alahdab, Fares},
  date = {2016-08-01},
  journaltitle = {BMJ Evidence-Based Medicine},
  volume = {21},
  number = {4},
  pages = {125--127},
  publisher = {Royal Society of Medicine},
  abstract = {{$<$}p{$>$}A pyramid has expressed the idea of hierarchy of medical evidence for so long, that not all evidence is the same. Systematic reviews and meta-analyses have been placed at the top of this pyramid for several good reasons. However, there are several counterarguments to this placement. We suggest another way of looking at the evidence-based medicine pyramid and explain how systematic reviews and meta-analyses are tools for consuming evidence—that is, appraising, synthesising and applying evidence.{$<$}/p{$>$}}
}

