@article{RigobonStoker2007,
  title = {{{ESTIMATION WITH CENSORED REGRESSORS}}: {{BASIC ISSUES}}*: Estimation with Censored Regressors: Basic Issues},
  shorttitle = {{{ESTIMATION WITH CENSORED REGRESSORS}}},
