@article{Matthaei1995,
  title = {The {{Sexual Division}} of {{Labor}}, {{Sexuality}}, and {{Lesbian}}/{{Gay Liberation}}: {{Toward}} a {{Marxist-Feminist Analysis}} of {{Sexuality}} in {{U}}.{{S}}. {{Capitalism}}},
