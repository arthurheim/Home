@online{DominiEtAl2020,
  type = {SSRN Scholarly Paper},
  title = {For {{Whom}} the {{Bell Tolls}}: {{The Effects}} of {{Automation}} on {{Wage}} and {{Gender Inequality Within Firms}}},
  shorttitle = {For {{Whom}} the {{Bell Tolls}}},
  author = {Domini, Giacomo and Grazzi, Marco and Moschella, Daniele and Treibich, Tania},
  date = {2020-10-02},
  number = {3701517},
  location = {Rochester, NY},
  abstract = {This paper investigates the impact of investment in automation- and AI- related goods on within-firm wage inequality in the French economy during the period 2002-2017. We document that most of wage inequality in France is accounted for by differences among workers belonging to the same firm, rather than by differences between sectors, firms, and occupations. Using an event-study approach on a sample of firms importing automation and AI-related goods, we find that automation/AI spikes are not followed by an increase in within-firm wage and gender inequality.  Instead, wages tend to increase at different percentiles of the distribution, revealing a relatively spread allocation of rents from automation/AI within the firm. This adds to previous findings showing picture of a `labor friendly' effect of the latest wave of new technologies.},
  pubstate = {prepublished}
}

@article{DonaldEtAl2014,
  title = {Testing the {{Unconfoundedness Assumption}} via {{Inverse Probability Weighted Estimators}} of ({{L}}){{ATT}}},
  author = {Donald, Stephen G. and Hsu, Yu-Chin and Lieli, Robert P.},
  date = {2014-07-03},
  journaltitle = {Journal of Business \& Economic Statistics},
  volume = {32},
  number = {3},
  pages = {395--415},
  publisher = {Taylor \& Francis},
  abstract = {We propose inverse probability weighted estimators for the local average treatment effect (LATE) and the local average treatment effect for the treated (LATT) under instrumental variable assumptions with covariates. We show that these estimators are asymptotically normal and efficient. When the (binary) instrument satisfies one-sided noncompliance, we propose a Durbin–Wu–Hausman-type test of whether treatment assignment is unconfounded conditional on some observables. The test is based on the fact that under one-sided noncompliance LATT coincides with the average treatment effect for the treated (ATT). We conduct Monte Carlo simulations to demonstrate, among other things, that part of the theoretical efficiency gain afforded by unconfoundedness in estimating ATT survives pretesting. We illustrate the implementation of the test on data from training programs administered under the Job Training Partnership Act in the United States. This article has online supplementary material.}
}

