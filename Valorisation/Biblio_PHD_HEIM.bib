  pages = {171--209},
  abstract = {L\&\#8217;objet de cet article est de donner \&\#224; voir la coh\&\#233;rence d\&\#8217;un chantier qu\&\#8217;on appelle commun\&\#233;ment la strat\&\#233;gie d\&\#8217;investissement social. Autour d\&\#8217;id\&\#233;es simples (concentrer les moyens sur la jeunesse et l\&\#8217;enfance) et de slogans ent\&\#234;tants (mieux vaut pr\&\#233;venir que gu\&\#233;rir), cette strat\&\#233;gie affiche des objectifs partiels sans parvenir \&\#224; valoriser ce que pourrait \&\#234;tre le chantier une fois achev\&\#233;. Cet article invitera donc \&\#224; le faire \&\#224; travers un questionnement sp\&\#233;cifique\&\#160;: le degr\&\#233; de coh\&\#233;rence de la perspective d\&\#8217;investissement social dans ses r\&\#233;f\&\#233;rentiels d\&\#8217;action. Ce niveau de r\&\#233;alit\&\#233; sera saisi \&\#224; travers les grands rapports prescripteurs mondiaux, r\&\#233;gionaux et locaux et \&\#224; travers la mise en forme des dispositifs de r\&\#233;formes en cours, notamment en France. L\&\#8217;argumentaire montre qu\&\#8217;aujourd\&\#8217;hui la perspective d\&\#8217;investissement social a fait son chemin, s\&\#8217;est frott\&\#233;e \&\#224; l\&\#8217;\&\#233;preuve des faits et s\&\#8217;est impos\&\#233;e au point de devenir une option s\&\#233;rieuse pour un imaginaire collectif alternatif.}
}

@article{NicolettiEtAl,
  title = {The {{Family Peer Effect}} on {{Mothers}}' {{Labour Supply}}},
  author = {Nicoletti, Cheti and Salvanes, Kjell G and Tominey, Emma},
  pages = {47},
  abstract = {The Family Peer Effect on Mothers’ Labour Supply* The documented historical rise in female labour force participation has flattened in recent decades, but the proportion of mothers working full-time has steadily increased. We provide the first empirical evidence that the increase in mothers’ working hours is amplified through the influence of family peers. Using Norwegian administrative data we study the long-run influence of the family network on mothers’ labour decisions up to seven years post birth. For identification, we exploit partially overlapping peer groups and assume that a mother interacts with her neighbours and family but not with her family’s neighbours. We explore mechanisms including information and imitation.}
}

@article{NicolettiEtAl2018,
  title = {The {{Family Peer Effect}} on {{Mothers}}' {{Labor Supply}}},
  author = {Nicoletti, Cheti and Salvanes, Kjell G. and Tominey, Emma},
  date = {2018-07},
  journaltitle = {American Economic Journal: Applied Economics},
  volume = {10},
  number = {3},
  pages = {206--234},
  abstract = {The historical rise in female labor force participation has flattened in recent decades, but the proportion of mothers working full-time has increased. We provide the first empirical evidence that the increase in mothers' working hours is amplified through the influence of family peers. For identification, we exploit partially overlapping peer groups. Using Norwegian administrative data, we find positive and statistically significant family peer effects but only on the intensive margin of women's labor supply. These are in part driven by concerns about time allocation from early childhood and concerns about earnings from age five.}
}

