@article{Bertram2007,
  title = {The {{Institutional Origins}} of “{{Workfarist}}” {{Social Policy}}},
  author = {Bertram, Eva C.},
  year = {2007/ed},
  journaltitle = {Studies in American Political Development},
  volume = {21},
  number = {2},
  pages = {203--229},
  publisher = {Cambridge University Press},
  abstract = {Between 1971 and 1975, congressional leaders quietly transformed the character and politics of public assistance in the United States. Three legislative initiatives were passed in quick succession and with little debate—the Talmadge Work Incentive amendments (WIN II), Supplemental Security Income (SSI), and the Earned Income Tax Credit (EITC). Although they drew little attention at the time, their combined impact was significant in two respects. First, by redefining the terms and target populations of income assistance, they established the elements of a workfarist approach to federal antipoverty policy, one that turned the ends and means of federal assistance away from traditional needs-based New Deal welfarism and toward the principle of rewarding, encouraging, and enforcing work. In addition, the initiatives helped to create the political capacity for subsequent retrenchment of traditional welfare programs, notably Aid to Families with Dependent Children (AFDC).}
}

