@article{AbadieEtAl,
  title = {Instrumental {{Variable Estimation}} with {{First-Stage Heterogeneity}}},
  author = {Abadie, Alberto and Gu, Jiaying and Shen, Shu},
  pages = {39},
  abstract = {We propose a simple data-driven procedure that exploits heterogeneity in the first stage correlation between an instrument and an endogenous variable to improve the efficiency of instrumental variable estimators. Efficiency improvements are important in the context of instrumental variable estimation because inferential procedures that are robust to weak instruments often generate uninformative confidence intervals in empirical applications. Our procedure either reduces the length of the confidence intervals for the second stage parameters or reduces the probability of producing unbounded confidence intervals. We show that the resulting efficiency gains can be quite large in settings where there is substantial heterogeneity in the first-stage parameters. We also show that a naive procedure used in some applied work, which consists of selecting the composition of the sample based on the value of the first-stage F -statistic, produces inconsistent estimators and may cause substantial over-rejection of a null hypothesis on a second stage parameter. Monte Carlo experiments show that the proposed method has excellent small sample performance. Finally, we apply the methods to study the return to compulsory schooling using census data in the United States. We demonstrate that our proposed method substantially tighten the confidence intervals of the return to compulsory education parameter, often from unbounded intervals generated by the classic Anderson-Rubin test to informative intervals.}
}

@article{AbadieEtAl2002,
  title = {Instrumental {{Variables Estimates}} of the {{Effect}} of {{Subsidized Training}} on the {{Quantiles}} of {{Trainee Earnings}}},
  author = {Abadie, Alberto and Angrist, Joshua and Imbens, Guido},
  date = {2002},
  journaltitle = {Econometrica},
  volume = {70},
  number = {1},
  pages = {91--117},
  publisher = {[Wiley, Econometric Society]},
  abstract = {This paper reports estimates of the effects of JTPA training programs on the distribution of earnings. The estimation uses a new instrumental variable (IV) method that measures program impacts on quantiles. The quantile treatment effects (QTE) estimator reduces to quantile regression when selection for treatment is exogenously determined. QTE can be computed as the solution to a convex linear programming problem, although this requires first-step estimation of a nuisance function. We develop distribution theory for the case where the first step is estimated nonparametrically. For women, the empirical results show that the JTPA program had the largest proportional impact at low quantiles. Perhaps surprisingly, however, JTPA training raised the quantiles of earnings for men only in the upper half of the trainee earnings distribution.}
}

@article{AbadieEtAl2010,
  title = {Synthetic {{Control Methods}} for {{Comparative Case Studies}}: {{Estimating}} the {{Effect}} of {{California}}’s {{Tobacco Control Program}}},
  shorttitle = {Synthetic {{Control Methods}} for {{Comparative Case Studies}}},
  author = {Abadie, Alberto and Diamond, Alexis and Hainmueller, Jens},
  date = {2010-06-01},
  journaltitle = {Journal of the American Statistical Association},
  volume = {105},
  number = {490},
  pages = {493--505},
  publisher = {Taylor \& Francis},
  abstract = {Building on an idea in Abadie and Gardeazabal (2003), this article investigates the application of synthetic control methods to comparative case studies. We discuss the advantages of these methods and apply them to study the effects of Proposition 99, a large-scale tobacco control program that California implemented in 1988. We demonstrate that, following Proposition 99, tobacco consumption fell markedly in California relative to a comparable synthetic control region. We estimate that by the year 2000 annual per-capita cigarette sales in California were about 26 packs lower than what they would have been in the absence of Proposition 99. Using new inferential methods proposed in this article, we demonstrate the significance of our estimates. Given that many policy interventions and events of interest in social sciences take place at an aggregate level (countries, regions, cities, etc.) and affect a small number of aggregate units, the potential applicability of synthetic control methods to comparative case studies is very large, especially in situations where traditional regression methods are not appropriate.}
}

@article{AbadieEtAl2011,
  title = {\textbf{Synth} : {{An}} {{\emph{R}}} {{Package}} for {{Synthetic Control Methods}} in {{Comparative Case Studies}}},
