@article{ValantWeixler2022,
  title = {Informing {{School-Choosing Families About Their Options}}: {{A Field Experiment From New Orleans}}},
  shorttitle = {Informing {{School-Choosing Families About Their Options}}},
  author = {Valant, Jon and Weixler, Lindsay H.},
  date = {2022-12-01},
  journaltitle = {Educational Evaluation and Policy Analysis},
  volume = {44},
  number = {4},
  pages = {608--637},
  publisher = {American Educational Research Association},
  abstract = {We conducted a randomized controlled trial to assess the effects of providing information to families as they choose schools. Likely applicants to prekindergarten, kindergarten, and ninth grade were assigned to one of three groups. A ?growth? group received lists (via U.S. mail, email, and text message) of the highest performing schools they could request. A ?distance? group received lists of schools in their home geographic zone. A ?control? group did not see any schools highlighted. The growth treatment led applicants to request more high-growth schools, with the strongest effects for high school choosers and families of students with disabilities. In addition, applicants? first-choice requests appeared less malleable than their lower ranked requests. The distance treatment had only modest effects.}
}

@article{Valiquette-TessierEtAl2019,
  title = {A {{Literature Review}} of {{Cultural Stereotypes Associated}} with {{Motherhood}} and {{Fatherhood}}},
  author = {Valiquette-Tessier, Sophie-Claire and Gosselin, Julie and Young, Marta and Thomassin, Kristel},
  date = {2019-05-19},
  journaltitle = {Marriage \& Family Review},
  shortjournal = {Marriage \& Family Review},
  volume = {55},
  number = {4},
  pages = {299--329},
  abstract = {In many societies, the iconic image of the family is that of a White, American middle-class, first-marriage nuclear family with two heterosexual parents and biological children. Research, however, has often neglected the complexity that exists within specific populations, particularly the impact of racial and ethnic diversity. The current review explores how motherhood and fatherhood, outside of the dominant North American cultural discourse, have been stereotyped in the research literature over the past decade. While results show few singularities in studies relating to fatherhood stereotypes, singularities in the findings associated with motherhood stereotypes are more abundant.}
}

@article{VanBelleEtAl2019,
