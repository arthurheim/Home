@article{HojmanShea,
  title = {The {{Opportunity Cost}} of {{Early Childhood Education}}: {{Formal}}, {{Informal}}, and {{Maternal Care}}},
  author = {Hojman, Andres and Shea, Joshua},
  pages = {18},
  abstract = {We decompose the total effect of a policy that gives free access to an early childhood education program on age 3 cognition, using a unique dataset with experimental and non-experimental sources of exogenous variation. We estimate a skill production function and find that two effects on cognition approximately offset each other: the positive effect of formal care and the negative effect of time lost with the mother, which we approximate by maternal labor supply. The magnitude of the policy’s positive effect on hours in formal care (12.63 additional hours/week) greatly exceeds its negative effect on maternal care (2.27 fewer hours/week). We implement a causal mediation analysis to decompose the positive and sizable total effect of the policy (0.44 SDs). The effect through formal care is 0.63 SDs; the effect through loss of maternal care is −0.12 SDs. The latter effect is much stronger for high-IQ mothers, suggesting that time-intensive programs should focus on disadvantaged children.}
}

