@report{VerlaatEtAl2020,
  type = {Working Paper},
  title = {Requirements vs. Autonomy: What Works in Social Assistance?},
  author = {Verlaat, Timo and Rosenkranz, Stephanie and Groot, Loek and Sanders, Mark},
  date = {2020-10},
  pages = {60},
  institution = {nders},
  location = {Utrecht University School of Economics (U.S.E.)},
  abstract = {Compliance requirements, such as active job search, in combination with monitoring and sanctions are a commonly used tool to induce e ort among social assistance claimants and achieve reemployment as quickly as possible. We conduct a randomised field experiment in the Netherlands to test two alternative regulatory approaches, which both build on providing claimants with more autonomy. For our first treatment we fully exempt claimants from requirements, monitoring and sanctions. For our second treatment claimants follow a one-on-one counselling programme shaped according to their needs and wishes. We find that compared to control, exempted claimants are twice as likely to have a job with earnings above the benefit level roughly two years later. Also, we observe a shift from temporary to permanent employment contracts under exemption. For counselling we find largely temporary labour market e ects. Using surveys, we find no evidence of e ects on outcomes related to health and well-being, social participation and job search behaviour for both treatments. We also analyse e ects on subgroup level.}
}

