@online{WhitemoreSchanzenbachStrain2020,
  title = {Employment {{Effects}} of the {{Earned Income Tax Credit}}: {{Taking}} the {{Long View}}},
  author = {Whitemore Schanzenbach, Diane and Strain, el R.},
  date = {2020-10},
  series = {{{NBER Working}} Paper Series},
  number = {28041},
  abstract = {The Earned Income Tax Credit (EITC) is the cornerstone U.S. anti-poverty program for families with children, typically lifting millions of children out of poverty each year. Targeted to lowincome households with children, and only available to those who work, the EITC contains strong incentives for non-workers to become employed. Most of the existing economics literature focuses on federal EITC expansions in the 1980s and 1990s. This paper takes a longer view, studying all federal expansions since the program’s inception in 1975. We find robust evidence that EITC expansions increase the extensive margin of labor supply.},
  pubstate = {prepublished}
}

