@article{LeroyEtAl2012,
  title = {The Impact of Daycare Programmes on Child Health, Nutrition and Development in Developing Countries: A Systematic Review},
  shorttitle = {The Impact of Daycare Programmes on Child Health, Nutrition and Development in Developing Countries},
  author = {Leroy, Jef L. and Gadsden, Paola and Guijarro, Maite},
  date = {2012-09-01},
  journaltitle = {Journal of Development Effectiveness},
  volume = {4},
  number = {3},
  pages = {472--496},
  publisher = {Routledge},
  abstract = {The authors conducted a systematic review of impact evaluations of daycare interventions on the health, nutrition and development of children under five in low-income and middle-income countries. The review was limited to intervention studies controlling for potential self-selection of beneficiaries. Papers were excluded based on study scope, type and quality. Only six studies (conducted in Latin America) met the inclusion criteria. Large positive effects on child development were found. Because of the limited evidence, no conclusions can be drawn regarding the impact on child health and nutrition. More rigorously conducted studies on the impact of daycare programmes in developing countries are needed.}
}

@incollection{Leruth2017,
  title = {France at a {{Crossroads}}: {{Societal Challenges}} to the {{Welfare State}} during {{Nicolas Sarkozy}}’s and {{François Hollande}}’s {{Presidential Terms}}},
  shorttitle = {France at a {{Crossroads}}},
  booktitle = {After {{Austerity}}: {{Welfare State Transformation}} in {{Europe}} after the {{Great Recession}}},
  author = {Leruth, Benjamin},
  editor = {Taylor-Gooby, Peter and Leruth, Benjamin and Chung, Heejung},
  date = {2017-08-10},
  pages = {0},
  publisher = {Oxford University Press},
  abstract = {France has established itself as one of the most ‘generous’ welfare states in the world. The Great Recession of 2007–8 confronted French social policy with escalating unemployment and deepening inequalities. Combined with major pension reforms, these led to strong levels of dissatisfaction across the country, exacerbated by tensions over immigration, Euroscepticism, and internal security problems. This chapter examines how these issues developed in political context and uses material from attitude surveys to analyse existing and future challenges for the welfare state in France. It assesses recent reforms: governments of right and left offered contrasting programmes but failed to win public trust. France now stands at a cross-roads, facing a strong presidential challenge from the anti-immigrant, anti-EU right.},
  isbn = {978-0-19-879026-6}
}

