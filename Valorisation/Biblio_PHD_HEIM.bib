@article{BhargavaLoewenstein2015,
  title = {Behavioral {{Economics}} and {{Public Policy}} 102: {{Beyond Nudging}}},
  shorttitle = {Behavioral {{Economics}} and {{Public Policy}} 102},
  author = {Bhargava, Saurabh and Loewenstein, George},
  date = {2015-05},
  journaltitle = {American Economic Review},
  volume = {105},
  number = {5},
  pages = {396--401},
  abstract = {Policymakers have recently embraced Behavioral Economics as an alternative approach which recognizes the limits and consequences of human decision-making.  Early applications of BE ("nudges") produced notable successes and helped to set the stage for more aggressive applications aimed at the deeper causes of policy problems.  We contend that policies that aspire to simplify products and incentives, rather than choice environments, aggressively protect consumers from behavioral exploitation, and leverage BE to enhance the design and implementation of traditional policy instruments offer solutions commensurate with contemporary challenges. Case studies in health insurance, privacy, and climate change illustrate the application of these ideas.}
}

