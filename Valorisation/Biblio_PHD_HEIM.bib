@article{AndreoniVesterlund2001,
  title = {Which Is the {{Fair Sex}}? {{Gender Differences}} in {{Altruism}}*},
  shorttitle = {Which Is the {{Fair Sex}}?},
  author = {Andreoni, James and Vesterlund, Lise},
  date = {2001-02-01},
  journaltitle = {The Quarterly Journal of Economics},
  shortjournal = {The Quarterly Journal of Economics},
  volume = {116},
  number = {1},
  pages = {293--312},
  abstract = {We study gender differences in altruism by examining a modified dictator game with varying incomes and prices. Our results indicate that the question “which is the fair sex?” has a complicated answer—when altruism is expensive, women are kinder, but when it is cheap, men are more altruistic. That is, we find that the male and female “demand curves for altruism” cross, and that men are more responsive to price changes. Furthermore, men are more likely to be either perfectly selfish or perfectly selfless, whereas women tend to be “equalitarians” who prefer to share evenly.}
}

@article{AndresenNix2019,
  title = {What {{Causes}} the {{Child Penalty}}? {{Evidence}} from {{Same Sex Couples}} and {{Policy Reforms}}},
  author = {Andresen, Martin Eckho and Nix, Emily},
  date = {2019},
  pages = {69},
  abstract = {Women in heterosexual couples experience signi cant reductions in labor market income following the birth of children, while their male partners experience no such income drops. This “relative child penalty” has been well documented and accounts for a signi cant amount of the gender income gap. In this paper we do two things. First, we use a simple household model to better understand the potential mechanisms driving the child penalty, which include gender norms around child care, female preferences for child care, e cient specialization within households, and the biological cost of giving birth. The model, combined with the estimated child penalties for heterosexual and same sex couples, suggests that the child penalty experienced by women in heterosexual couples is primarily explained by female preferences for child care and gender norms, with a smaller contribution due to the biological costs of giving birth. Second, we provide causal estimates on the impact of two family policies aimed at reducing the relative child penalty: paternity leave and subsidized early child care. Our precise and robust regression discontinuity results show no signi cant impact of paternity leave use on the relative child penalty. Early subsidized care seems to have more promise as a policy tool for a ecting child penalties, as we nd a 25\% reduction in child penalties per year of child care use from a large Norwegian reform that expanded access to child care.}
}

