@article{ComptonPollak2014,
  title = {Family Proximity, Childcare, and Women’s Labor Force Attachment},
  author = {Compton, Janice and Pollak, Robert A.},
  date = {2014-01},
  journaltitle = {Journal of Urban Economics},
  shortjournal = {Journal of Urban Economics},
  volume = {79},
  pages = {72--90},
  abstract = {We show that close geographical proximity to mothers or mothers-in-law has a substantial positive effect on the labor supply of married women with young children. We argue that the mechanism through which proximity increases labor supply is the availability of childcare. We interpret availability broadly enough to include not only regular scheduled childcare during work hours but also an insurance aspect of proximity (e.g., a mother or mother-in-law who can to provide irregular or unanticipated childcare). Using two large datasets, the National Survey of Families and Households and the public use files of the U.S. Census, we find that the predicted probability of employment and labor force participation is 4–10 percentage points higher for married women with young children living in close proximity to their mothers or their mothers-in-law compared with those living further away.}
}

@article{ConleyHeerwig2011,
  title = {The {{War}} at {{Home}}: {{Effects}} of {{Vietnam-Era Military Service}} on {{Postwar Household Stability}}},
  shorttitle = {The {{War}} at {{Home}}},
  author = {Conley, Dalton and Heerwig, Jennifer},
  date = {2011-05},
  journaltitle = {American Economic Review},
  volume = {101},
  number = {3},
  pages = {350--354},
  abstract = {Prior researchers have deployed the Vietnam-era draft lottery as an instrument to estimate causal effects of military service on health and earnings. However, household and residential outcomes may be more sensitive to the psychological effects of military service. Using 2SLS analyses of the 2000 Census and the 2005 American Community Survey, we find mixed results for residential stability, housing tenure, and extended family residence. While in the ACS white veterans are less mobile, veteran status has no effect on homeownership. Veteran status reduces extended family living for whites in the Census but increases it for ACS veterans of "other" races.}
}

