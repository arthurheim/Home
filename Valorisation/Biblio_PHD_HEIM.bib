  author = {Mazières-Vaysse, Adrien},
  date = {2020-06-01},
  journaltitle = {Sciences de la société},
  number = {105},
  publisher = {Presses Universitaires du Mirail},
  abstract = {La mise en place du revenu de solidarité active (rsa) en France, qui s’inscrit dans un tournant européen de développement des politiques d’activation, a conduit à un transfert de catégories des politiques publiques dans les identités individuelles des allocataires. Les mouvements sociaux en lutte contre la précarité s’opposent à la diffusion de ces identités stigmatisées et cherchent à faire émerger, à travers la critique du référentiel d’activation, des contre-identités et des cadres d’interprétation alternatifs. Cet article analyse la production de ces contre-identités qui s’appuient sur une variété de discours militants contre la précarité et l’activation~: défense et volonté d’extension de la logique assurantielle de la protection sociale dans un pôle d’inspiration syndicale~; recherche de l’égalité des droits démocratiques appuyée sur une philosophie de la justice sociale dans les organisations de jeunes précaires~; déconnexion entre droit au revenu et obligation de travail dans un pôle mettant les précaires au cœur de la transformation sociale.},
  issue = {105}
}

