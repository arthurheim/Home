  abstract = {Women empowerment and economic development are closely related: in one direction, development alone can play a major role in driving down inequality between men and women; in the other direction, empowering women may benefit development. Does this imply that pushing just one of these two levers would set a virtuous circle in motion? This paper reviews the literature on both sides of the empowerment—development nexus, and argues that the interrelationships are probably too weak to be self-sustaining, and that continuous policy commitment to equality for its own sake may be needed to bring about equality between men and women. (JEL I14, I24, I32, I38, J13, J16, O15)}
}

@article{Duflo2017,
  title = {The {{Economist}} as {{Plumber}}},
  author = {Duflo, Esther},
  date = {2017-05},
  journaltitle = {American Economic Review},
  volume = {107},
  number = {5},
  pages = {1--26},
  abstract = {As economists increasingly help governments design new policies and regulations, they take on an added responsibility to engage with the details of policy making and, in doing so, to adopt the mindset of a plumber. Plumbers try to predict as well as possible what may work in the real world, mindful that tinkering and adjusting will be necessary since our models gives us very little theoretical guidance on what (and how) details will matter. This essay argues that economists should seriously engage with plumbing, in the interest of both society and our discipline.}
}

