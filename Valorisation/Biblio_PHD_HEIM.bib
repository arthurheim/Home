@article{BlandenEtAl2022,
  title = {Quantity and Quality of Childcare and Children’s Educational Outcomes},
  author = {Blanden, Jo and Del Bono, Emilia and Hansen, Kirstine and Rabe, Birgitta},
  date = {2022-04-01},
  journaltitle = {Journal of Population Economics},
