@article{DuchiniEffenterre2022,
  title = {School {{Schedule}} and the {{Gender Pay Gap}}},
  author = {Duchini, Emma and Effenterre, Clémentine Van},
  date = {2022-03-09},
  journaltitle = {Journal of Human Resources},
  publisher = {University of Wisconsin Press},
  abstract = {We provide causal evidence that children’s school schedules contribute to the persistence of the gender pay gap between parents. Historically, French children have had no school on Wednesdays. In 2013, a reform reallocated some classes to Wednesday mornings. Exploiting variations in the application of this reform over time and age of the youngest child, we show that mothers are more likely to adopt a regular Monday-Friday full-time working schedule after the reform, while fathers’ labor supply is unchanged. Consequently, the reform decreased the gender gap in monthly wages by 6 percent, generating welfare benefits that substantially outweigh its costs.}
}

@article{Duflo,
  title = {The {{Economist}} as {{Plumber}}},
  author = {Duflo, Esther},
  pages = {41},
  abstract = {As economists increasingly help governments design new policies and regulations, they take on an added responsibility to engage with the details of policy making and, in doing so, to adopt the mindset of a plumber. Plumbers try to predict as well as possible what may work in the real world, mindful that tinkering and adjusting will be necessary since our models gives us very little theoretical guidance on what (and how) details will matter. This essay argues that economists should seriously engage with plumbing, in the interest of both society and our discipline.}
}

