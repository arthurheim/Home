@article{GarciaEtAl,
  ids = {GarciaEtAla},
  title = {The {{Opportunity Cost}} of {{Early Childhood Education}}: {{Formal}}, {{Informal}}, and {{Maternal Care}}⇤},
  author = {Garcıa, Jorge Luis and Hojman, Andres and Shea, Joshua},
  pages = {22},
  abstract = {We decompose the total e↵ect of a policy that gives free access to an early childhood education program on age 3 cognition, using a unique dataset with experimental and non-experimental sources of exogenous variation. We estimate a skill production function and find that two e↵ects on cognition approximately o↵set each other: the positive e↵ect of formal care and the negative e↵ect of time lost with the mother, which we approximate by maternal labor supply. The magnitude of the policy’s positive e↵ect on hours in formal care (12.63 additional hours/week) greatly exceeds its negative e↵ect on maternal care (2.27 fewer hours/week). We implement a causal mediation analysis to decompose the positive and sizable total e↵ect of the policy (0.44 SDs). The e↵ect through formal care is 0.63 SDs; the e↵ect through loss of maternal care is 0.12 SDs. The latter e↵ect is much stronger for high-IQ mothers, suggesting that time-intensive programs should focus on disadvantaged children.}
}

@article{GarciaEtAl2019,
  title = {Quantifying the {{Life-Cycle Benefits}} of an {{Influential Early Childhood Program}}},
  author = {Garcia, Jorge Luis and Heckman, James J. and Leaf, Duncan Ermini and Prados, Maria Jose},
  date = {2019-08-02},
  journaltitle = {Journal of Political Economy},
  shortjournal = {Journal of Political Economy},
  pages = {705718},
  abstract = {This paper quantifies and aggregates the multiple lifetime benefits of an influential highquality early childhood program with outcomes measured through midlife. Guided by economic theory, we supplement experimental data with non-experimental data to forecast the life-cycle benefits and costs of the program. Our point estimate of the internal rate of return is 13.7\% with an associated benefit/cost ratio of 7.3. We account for model estimation and forecasting error and present estimates from extensive sensitivity analyses. This paper is a template for synthesizing experimental and nonexperimental data using economic theory to estimate the long-run life-cycle benefits of social programs.}
}

