@report{Heim2013,
  type = {Mémoire de Master sous la direction d'Eric Maurin},
  title = {Public Sector as a Shelter ? {{The}} Impact of Graduating in a Recession on Selection into Public Employment.},
  author = {Heim, Arthur},
  date = {2013-09},
  pages = {64},
  abstract = {In this paper, we investigate the impact of bad economic conditions at the entry in the labour market on the selection into the public sector in France. Using aggregated data at cohort level of young graduates from 1982 to 2011, we find that youth unemployment rate at graduation increases the share of civil servant among all affected men cohorts and women cohorts with more than high school degree. However, the share of civil servant in women cohorts with low education is significantly lower. The effect fades out within 4 years for men but remains significant for women at least 7 years. These estimates reveal the attractiveness of the French public sector in adverse economic conditions. Yet since the number of vacancies is very limited, our results confirm a change in the composition of new entrants, where women with little education are permanently evicted for the benefit of more men and more educated young graduates. The fading effect for men cohorts may have two causes: Men either shift back to their original career strategy. Or, other cohorts suffer from subsequent shocks or unemployment hysteresis and also joins the public sector, making inter-cohort differences null.}
}

