@article{NollenbergerRodriguez-Planas2015,
  title = {Full-Time Universal Childcare in a Context of Low Maternal Employment: {{Quasi-experimental}} Evidence from {{Spain}}},
  shorttitle = {Full-Time Universal Childcare in a Context of Low Maternal Employment},
  author = {Nollenberger, Natalia and Rodríguez-Planas, Núria},
  date = {2015-10},
  journaltitle = {Labour Economics},
  shortjournal = {Labour Economics},
  volume = {36},
  pages = {124--136},
  abstract = {Using a natural experiment framework, we study the effects of offering full-time public childcare for 3-year-olds in a context of low female labor force participation and insufficient infrastructure of childcare slots. We find that two mothers entered employment for every ten additional children enrolled in public childcare. The effect is driven by mothers 30 years old and older and those with two children or more. While our estimates compare to those found earlier, they cannot be explained by a crowding out of alternative childcare modes. Nonetheless, as the reform was implemented in a period of low labor demand in Spain, our estimates may not be as modest at they appear at first sight.}
}

