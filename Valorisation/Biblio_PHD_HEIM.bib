@article{CornelsonSiow2016,
  title = {A {{Quantitative Review}} of {$<$}em{$>$}{{Marriage Markets}}: {{How Inequality Is Remaking}} the {{American Family}}{$<$}/Em{$>$} by {{Carbone}} and {{Cahn}}},
  shorttitle = {A {{Quantitative Review}} of {$<$}em{$>$}{{Marriage Markets}}},
  author = {Cornelson, Kirsten and Siow, Aloysius},
  date = {2016-03},
  journaltitle = {Journal of Economic Literature},
  volume = {54},
  number = {1},
  pages = {193--207},
  abstract = {June Carbone and Naomi Cahn argue that growing earnings inequality and the increased educational attainment of women, relative to men, have led to declining marriage rates for less-educated women and an increase in positive assortative matching since the 1970s. These trends have negatively affected the welfare of children, as they increase the proportion of poor, single-female-headed households. Using data on marriage markets defined by state, race and time, and the Choo-Siow marriage matching function, this review provides a quantitative assessment of these claims. We show that changes in earnings inequality had a qualitatively consistent but modest quantitative impact on marriage rates and positive assortative matching. Neither changes in the wage distributions nor educational attainments can explain the large decline in marriage rates over this period. (JEL C78, D63, J12, J15, J16, J31)}
}

