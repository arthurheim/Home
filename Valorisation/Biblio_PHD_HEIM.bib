@article{Uunk2004,
  title = {The {{Economic Consequences}} of {{Divorce}} for {{Women}} in the {{European Union}}: {{The Impact}} of {{Welfare State Arrangements}}},
  shorttitle = {The {{Economic Consequences}} of {{Divorce}} for {{Women}} in the {{European Union}}},
  author = {Uunk, Wilfred},
  date = {2004},
  journaltitle = {European Journal of Population / Revue Européenne de Démographie},
  volume = {20},
  number = {3},
  pages = {251--285},
  publisher = {Springer},
  abstract = {Increasing divorce rates and growing concerns on welfare dependency urge the question whether welfare state arrangements moderate the negative economic consequences of divorce. In this study, the question is answered by comparing the short-term economic consequences of divorce for women across 14 Member States of the European Union. Using longitudinal data from the 1994-2000 European Community Household Panel survey, 1 demonstrate that the Member States differ in the extent to which women suffer economically from divorce. Multivariate analyses show that welfare state arrangements temper the economic consequences of divorce for women. Income-related arrangements reduce the economic strains of divorce most, then employment-related arrangements. These welfare state effects cannot be attributed to country differences in the composition of divorced women. /// L'accroissement des taux de divorce et la menace grandissante qui pèse sur l'aide sociale rendent de plus en plus urgente la question de savoir si les avantages accordés par l'étatprovidence peuvent modérer les conséquences économiques négatives du divorce. Cet article répond à cette question en comparant les conséquences économiques à court terme du divorce pour les femmes dans 14 états membres de l'Union européenne. L'analyse des données longitudinales du panel européen des ménages de 1994 à 2000 révèle que les effets économiques du divorce pour les femmes n'ont pas la même ampleur dans tous les pays. Les analyses multivariées montrent que les mesures mises en place dans les états-providences tempèrent ces effets. Les compensations salariales ont l'effet le plus important, suivies par les aides à l'emploi. Ces effets ne peuvent être mis sur le compte de différences de structure dans le groupe des femmes divorcées d'un pays à l'autre.}
}

