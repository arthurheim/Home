@article{White2009,
  title = {Explaining {{Differences}} in {{Child Care Policy Development}} in {{France}} and the {{USA}}: {{Norms}}, {{Frames}}, {{Programmatic Ideas}}},
  shorttitle = {Explaining {{Differences}} in {{Child Care Policy Development}} in {{France}} and the {{USA}}},
  author = {White, Linda A.},
  date = {2009},
  journaltitle = {International Political Science Review / Revue internationale de science politique},
  volume = {30},
  number = {4},
  pages = {385--405},
  publisher = {Sage Publications, Ltd.},
  abstract = {This article provides an answer to the question of why government support for child care policies and programs in the United States remains weak, despite increasing levels of women's labor market participation and a relatively strong women's movement, but strong in France, a country that has had lower levels of women's labor market participation as well as a much weaker women's movement. While those differences can be explained in part by economic and political interests and institutional and broad cultural differences, a theoretically richer understanding emerges when one examines how the three kinds of ideas underpinning these policy choices — norms, frames, and programmatic ideas — helped shape policies that emerged within specific actor, institutional, and cultural contexts. Cet article explique pourquoi le soutien gouvernemental aux politiques et aux programmes en direction de l'enfance aux États-Unis reste faible, malgré une participation accrue des femmes au marché du travail et un mouvement de femmes relativement fort, mais est élevé en France, un pays aux niveaux inférieurs de participation des femmes au marché du travail et avec un mouvement de femmes beaucoup plus faible. Bien qu'on puisse en partie expliquer ces dissemblances par des divergences d'intérêts économiques et politiques, ainsi que par des différences institutionnelles et largement culturelles, une compréhension théorique plus riche apparaît lorsqu'on examine comment les trois types d'idées sous-jacentes à ces choix de politiques publiques — les normes, les frames, et les idées programmatiques — contribuent à produire les politiques au sein d'acteurs et de contextes institutionnels et culturels spécifiques. El artículo ofrece una respuesta a la cuestión de por qué el apoyo gubernamental a las políticas y programas de cuidado infantil continúa siendo débil en Estados Unidos — a pesar del aumento de la participación femenina en el mercado laboral y de la existencia de un movimiento de mujeres bastante robusto — y es, sin embargo, fuerte en un país como Francia, donde la tasa de participación femenina en el mercado laboral es más baja, y donde el movimiento de mujeres es mucho más débil que en EEUU. Si bien dichas variaciones pueden explicarse en base a diferencias de intereses económicos y políticos, así como diferencias institucionales y culturales entre los dos países, el artículo presenta una explicación más rica desde el punto de vista teórico. Dicha explicación está basada en un examen de cómo tres categorías de ideas que informan estas opciones políticas — normas, marcos, e ideas programáticas — dan forma a políticas emergentes en contextos de actores, institucionales y culturales específicos.}
}

@article{White2019,
  title = {The Twenty-First Century Experimenting Society: The Four Waves of the Evidence Revolution},
  shorttitle = {The Twenty-First Century Experimenting Society},
  author = {White, Howard},
  date = {2019-12},
  journaltitle = {Palgrave Communications},
  shortjournal = {Palgrave Commun},
  volume = {5},
  number = {1},
  pages = {47},
  abstract = {This paper presents a personal perspective–drawing especially on the author’s experience in international development—of the evidence revolution, which has unfolded in fours waves over the last 30 years: (1) the results agenda as part of New Public Management in the 1990s, (2) the rise of impact evaluations, notably randomized controlled trials (RCTs) since the early 2000s, (3) increased production of systematic reviews over the last ten years, and (4) moves to institutionalize the use of evidence through the emergence of knowledge brokering agencies, most notably the What Works movement in the United States and the United Kingdom. A fifth wave may come from the potential from AI, machine learning and Big Data. Each successive wave has built on the last, and together they comprise the supply side of the evidence architecture. To support the use of evidence demand side activities such as Evidence Needs Assessments and Use of Evidence Awards are proposed.}
}

