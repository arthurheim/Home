@article{MacKinnonWebb2020,
  title = {Randomization Inference for Difference-in-Differences with Few Treated Clusters},
  author = {MacKinnon, James G. and Webb, Matthew D.},
  date = {2020-05},
  journaltitle = {Journal of Econometrics},
  shortjournal = {Journal of Econometrics},
  pages = {S0304407620301445},
  abstract = {Inference using difference-in-differences with clustered data requires care. Previous research has shown that, when there are few treated clusters, t-tests based on clusterrobust variance estimators (CRVEs) severely overreject, and different variants of the wild cluster bootstrap can either overreject or underreject dramatically. We study two randomization inference (RI) procedures. A procedure based on estimated coefficients may be unreliable when clusters are heterogeneous. A procedure based on t-statistics typically performs better (although by no means perfectly) under the null, but at the cost of some power loss. An empirical example demonstrates that RI procedures can yield inferences that differ dramatically from those of other methods.}
}

@article{Maclean2013,
  title = {The Health Effects of Leaving School in a Bad Economy},
  author = {Maclean, Johanna Catherine},
  date = {2013-09-01},
  journaltitle = {Journal of Health Economics},
  shortjournal = {Journal of Health Economics},
  volume = {32},
  number = {5},
  pages = {951--964},
  abstract = {This study investigates the lasting health effects of leaving school in a bad economy. Three empirical patterns motivate this study: Leaving school in a bad economy has persistent and negative career effects, career and health outcomes are correlated, and fluctuations in contemporaneous economic conditions affect health in the short-run. I draw data from the National Longitudinal Survey of Youth 1979 Age 40 Health Supplement. Members of my sample left school between 1976 and 1992. I find that men who left school when the school-leaving state unemployment rate was high have worse health at age 40 than otherwise similar men, while leaving school in a bad economy lowers depressive symptoms at age 40 among women. A 1 percentage point increase in the school-leaving state unemployment rate leads to a 0.5\% to 18\% reduction in the measured health outcomes among men and a 6\% improvement in depressive symptoms among women.}
}

