@article{VandenbrouckeVleminckx2011,
  title = {Disappointing Poverty Trends: Is the Social Investment State to Blame?},
  shorttitle = {Disappointing Poverty Trends},
  author = {Vandenbroucke, Frank and Vleminckx, Koen},
  date = {2011-12},
  journaltitle = {Journal of European Social Policy},
  shortjournal = {Journal of European Social Policy},
  volume = {21},
  number = {5},
  pages = {450--471},
  abstract = {Should we explain the disappointing outcomes of the Open Method of Co-ordination on Inclusion by methodological weaknesses or by substantive contradictions in the ‘social investment’ paradigm? To clarify the underlying concepts, we first revisit the original ‘Lisbon inspiration’ and then relate it to the idea of the ‘new welfare state’, as proposed in the literature on new risks in postindustrial societies. We then discuss two explanations for disappointing poverty trends, suggested by critical accounts of the ‘social investment state’: ‘resource competition’ and a ‘re-commodification’. We do not find these explanations convincing per se and conclude that the jury is still out on the ‘social investment state’. However, policy-makers cannot ignore the failure of employment policies to reduce the proportion of children and working-age adults living in jobless households in the EU, and they should not deny the reality of a ‘trilemma of activation’. Finally, we identify policy conditions that may facilitate the complementarity of social investment and social inclusion.}
}

